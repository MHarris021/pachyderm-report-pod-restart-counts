{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "sourceMap": true,
        "outDir": "build",
        "rootDir": ".",
        "strict": true,
        "esModuleInterop": true,
    },
    "include": [
        "src/**/*",
    ],
    "exclude": [
        "node_modules",
        "build",
    ],
    "paths": {
        "@models/*": ["src/models/*"],
        "@constants/*": ["src/constants/*"],
        "@test/*": ["test/*"],
    },

}